{
    "source": {
        "include": [
            ""
        ],
        "includePattern": "classes/*",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/docdash",
        "theme_opts": {
            "theme": "dark"
        }
    }
}